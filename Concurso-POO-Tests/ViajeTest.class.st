Class {
	#name : #ViajeTest,
	#superclass : #TestCase,
	#category : #'Concurso-POO-Tests-Base'
}

{ #category : #'test methods' }
ViajeTest >> testTieneDestino [

	| v |
	v := Viaje conNumeroDestino: 1 destino: 'Cutral c贸'.

	self assert: (v tieneDestino: 'Cutral c贸')
]

{ #category : #tests }
ViajeTest >> testViajeIsSetAndRead [

	| v |
	v := Viaje conNumeroDestino: 1 destino: 'Cutral c贸'.
	self assert: v destino equals: 'Cutral c贸'
]
